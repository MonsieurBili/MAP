<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controller.MainController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
    <TabPane xmlns:fx="http://javafx.com/fxml">
        <tabs>
            <Tab text="Ducks" closable="false">
                <fx:include fx:id="duckView" source="duckView.fxml" />
            </Tab>
            <Tab text="Persons" closable="false">
                <fx:include fx:id="personView" source="personView.fxml" />
            </Tab>
            <Tab text="Friendships" closable="false">
                <fx:include fx:id="friendshipView" source="friendshipView.fxml"/>
            </Tab>
            <Tab text="Chat" closable="false">
                <fx:include fx:id="chatView" source="chatView.fxml"/>
            </Tab>
        </tabs>
    </TabPane>
    </children>
</AnchorPane>
